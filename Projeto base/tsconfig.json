{
  "compilerOptions": {
    /* Language and Environment*/
    "target": "es2020",
    /* Modules */
    "module": "commonjs",
    /* Interop Constraints */
    "preserveConstEnums": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    /* Type Checking */
    "strict": true,
    "skipLibCheck": true
  }
}
